{
    "name": "spatie/laravel-mailable-test",
    "description": "An artisan command to easily test a mailable",
    "keywords": [
        "spatie",
        "laravel-mailable-test"
    ],
    "homepage": "https://github.com/spatie/laravel-mailable-test",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailableTest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailableTest\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  }
}
